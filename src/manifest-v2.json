{
    "manifest_version": 2,
    "name": "Blackboard Ultra Extended",
    "version": "1.0",
    "description": "Multitool browser add-on for Blackboard Learn Ultra",
    "author": "Douwe Somers",
    "background": {
        "scripts": [
            "module-loader.js"
        ]
    },
    "permissions": [
        "scripting",
        "*://*.api.blackboard.com/*",
        "*://*/ultra/*"
    ]
}