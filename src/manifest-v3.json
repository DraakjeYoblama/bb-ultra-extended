{
    "manifest_version": 3,
    "name": "Blackboard Ultra Extended",
    "version": "1.0",
    "description": "Multitool browser add-on for Blackboard Learn Ultra",
    "author": "DraakjeYoblama",
    "content_scripts": [
        {
            "matches": [
                "*://*.api.blackboard.com/*",
                "*://*/ultra/*"
            ],
            "js": [
                "main.js"
            ],
            "all_frames": true
        }
    ]
}