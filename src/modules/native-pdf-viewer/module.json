{
    "name": "Native PDF Viewer",
    "description": "Replaces the built-in PDF viewer with the browser default. \nBased on an extension by Tarik Kirgin.",
    "contributors": [
        "Tarik Kirgin",
        "Douwe Somers"
    ],
    "options": {
        "enable": true
    },
    "contentScriptParams": {
        "id": "native-pdf-viewer",
        "matches": [
            "*://*.api.blackboard.com/*",
            "*://*/ultra/*"
        ],
        "allFrames": true,
        "js": [
            "native-pdf-viewer.js"
        ]
    }
}