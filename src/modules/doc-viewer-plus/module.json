{
    "name": "Doc Viewer+",
    "description": "Extends the built-in PDF viewer. \nBased on an extension by Raj Kundu.",
    "contributors": [
        "Raj Kundu",
        "Douwe Somers"
    ],
    "options": {
        "enable": true
    },
    "contentScriptParams": {
        "id": "doc-viewer-plus",
        "matches": [
            "*://*.api.blackboard.com/*",
            "*://*/ultra/*"
        ],
        "allFrames": true,
        "js": [
            "doc-viewer-plus.js"
        ]
    }
}