{
    "name": "Example Module",
    "description": "This is an example module. It adds a red border to the page to show the plugin is working. It also gives an example of how to use the module.json file.",
    "contributors": [
        "Douwe Somers"
    ],
    "icon": "icon.png",
    "options": {
        "color-id": "colorpicker",
        "text-id": "text",
        "image-id": "image"
    },
    "contentScriptParams": {
        "id": "example",
        "matches": [
            "*://*.api.blackboard.com/*",
            "*://*/ultra/*"
        ],
        "allFrames": true,
        "js": [
            "example.js"
        ],
        "css": [
            "example.css"
        ]
    }
}